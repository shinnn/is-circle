{
  "name": "is-circleci",
  "version": "1.0.0",
  "description": "Check if a script is running on CircleCI",
  "repository": "shinnn/is-circleci",
  "scripts": {
    "pretest": "eslint --config @shinnn/node index.js test.js",
    "test": "node --strong_mode --throw-deprecation --track-heap-objects test.js",
    "test-circleci": "node --strong_mode --throw-deprecation --track-heap-objects node_modules/.bin/istanbul cover test.js"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "keywords": [
    "env",
    "environment",
    "check",
    "checker",
    "if",
    "whether",
    "boolean",
    "true",
    "false",
    "ci",
    "circleci"
  ],
  "devDependencies": {
    "@shinnn/eslint-config-node": "^1.0.1",
    "eslint": "^1.10.3",
    "istanbul": "^0.4.2",
    "tape": "^4.4.0"
  }
}
